@Scripts.Render("~/bundles/jQuery")
@model DotNet.Highcharts.Highcharts

<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/Highcharts-4.0.1/js/highcharts.js"></script>
@*@model IEnumerable<MonthlyBudgetCalculator.Models.Budget>*@
@{
    ViewBag.Title = "Forecast";
}
<p>
    <br />
    <a class="btn btn-primary btn-xs" @Html.ActionLink("Home", "Index", "Home")</a>
    <a class="btn btn-primary btn-xs" @Html.ActionLink("Application Users", "Index", "BudgetUsers")</a>
</p>
<h5 class="text-info">Budget Analysis Charts</h5>
<h6>*Analysis on Income, Expenditure and Budget Balance</h6>
<h5>Expenditure Forecast for Car Expenses based on the last @ViewBag.size budgets, is €@ViewBag.carAverage</h5>
<h5>Expenditure Forecast for Household Expenses based on the last @ViewBag.size budgets, is €@ViewBag.householdAverage</h5>
<h5>Expenditure Forecast for Personal Expenses based on the last @ViewBag.size budgets, is €@ViewBag.personalAverage</h5>
<h5>Expenditure Forecast for Travel Expenses based on the last @ViewBag.size budgets, is €@ViewBag.travelAverage</h5>
<h5>Expenditure Forecast for Utility Bill Expenses based on the last @ViewBag.size budgets, is €@ViewBag.utilityBillAverage</h5>





@(Model)


